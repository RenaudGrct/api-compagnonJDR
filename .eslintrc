{
  "env": {
  "browser": true,
  "es6": true,
  "node": true
},
"parserOptions": {
  "ecmaVersion": 2020
},
"extends": "eslint:recommended",
"rules": {
  "indent": ["error", 4],
  "quotes": ["error", "double"],
  "semi": ["error", "always"],
  "no-multi-spaces": "error",
  "no-trailing-spaces": "error"
}
}